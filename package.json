{
  "name": "webapp",
  "version": "0.1.0",
  "description": "Web Apps",
  "author": "peaceiris",
  "private": true,
  "scripts": {
    "dev": "run-p client nuxt",
    "nuxt": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "client": "wait-on http://localhost:3000 && open http://localhost:3000",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "prettier --write **/*.{js,vue}",
    "format:check": "prettier --check **/*.{js,vue}",
    "test": "jest --coverage --verbose",
    "release": "standard-version",
    "update-deps": "(git diff 'HEAD@{1}' --name-only | grep 'package-lock.json' > /dev/null) && npm ci || :"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint",
      "jest --passWithNoTests"
    ]
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm run update-deps; git remote prune origin"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "nuxt": "^2.14.5"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/proxy": "^2.0.1",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.2.1",
    "prettier": "^2.0.5",
    "vue-jest": "^4.0.0-beta.4",
    "vue-property-decorator": "^9.0.0",
    "wait-on": "^5.2.0"
  }
}
